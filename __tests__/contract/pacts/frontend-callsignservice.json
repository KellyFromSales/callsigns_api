{
  "consumer": {
    "name": "Frontend"
  },
  "provider": {
    "name": "CallsignService"
  },
  "interactions": [
    {
      "description": "a request for a callsign",
      "providerState": "i have a list of callsigns",
      "request": {
        "method": "GET",
        "path": "/callsigns/M7RFT",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "Callsign": [
            {
              "Callsign": "M7RFT",
              "Status": "Allocated",
              "Type": "Call Sign - Amateur",
              "Level": "Foundation",
              "QRZ": "https://www.qrz.com/db/M7RFT"
            }
          ]
        },
        "matchingRules": {
          "$.body.Callsign": {
            "min": 1
          },
          "$.body.Callsign[*].*": {
            "match": "type"
          },
          "$.body.Callsign[*].Callsign": {
            "match": "type"
          },
          "$.body.Callsign[*].Status": {
            "match": "type"
          },
          "$.body.Callsign[*].Type": {
            "match": "type"
          },
          "$.body.Callsign[*].Level": {
            "match": "type"
          },
          "$.body.Callsign[*].QRZ": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request for all callsigns",
      "providerState": "i have a list of callsigns",
      "request": {
        "method": "GET",
        "path": "/callsigns",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "Callsigns": [
            {
              "Callsign": "M7RFT"
            }
          ]
        },
        "matchingRules": {
          "$.body.Callsigns": {
            "min": 1
          },
          "$.body.Callsigns[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}